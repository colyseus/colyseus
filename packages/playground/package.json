{
  "name": "@colyseus/playground",
  "version": "0.17.3",
  "main": "./build/index.js",
  "module": "./build/index.mjs",
  "typings": "./build/index.d.ts",
  "exports": {
    ".": {
      "@source": "./src-backend/index.ts",
      "types": "./build/index.d.ts",
      "import": "./build/index.mjs",
      "require": "./build/index.js"
    },
    "./*": {
      "@source": "./src-backend/*.ts",
      "types": "./build/*.d.ts",
      "import": "./build/*.mjs",
      "require": "./build/*.js"
    }
  },
  "scripts": {
    "start": "vite",
    "build": "vite build && node build.mjs",
    "preview": "vite preview",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "build",
    "src-backend"
  ],
  "peerDependencies": {
    "@colyseus/auth": "workspace:^",
    "@colyseus/core": "workspace:^",
    "express": ">=4.16.0",
    "zod": "^4.1.12"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@colyseus/sdk": "workspace:^",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/jest": "^27.5.2",
    "@types/jsoneditor": "^9.9.0",
    "@types/node": "^16.18.31",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "esbuild": "^0.17.19",
    "fast-glob": "^3.2.12",
    "jsoneditor": "^9.10.2",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flatpickr": "^3.10.13",
    "react-router-dom": "^6.11.2",
    "react-syntax-highlighter": "^16.1.0",
    "react-tailwindcss-select": "^1.8.5",
    "react-transition-group": "^4.4.5",
    "react18-json-view": "^0.0.8",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.9.3",
    "vite": "^5.0.11",
    "vitest": "^1.1.3",
    "web-vitals": "^2.1.4"
  },
  "dependencies": {
    "@colyseus/better-call": "workspace:^"
  }
}
