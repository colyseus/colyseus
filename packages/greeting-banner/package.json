{
  "name": "@colyseus/greeting-banner",
  "version": "3.0.0",
  "type": "module",
  "description": "Greeting banner for Colyseus & Cloud",
  "input": "./src/index.ts",
  "main": "./build/index.js",
  "module": "./src/index.ts",
  "typings": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
