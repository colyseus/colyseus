{
  "name": "@colyseus/greeting-banner",
  "version": "3.0.6",
  "type": "module",
  "description": "Greeting banner for Colyseus & Cloud",
  "input": "./src/index.ts",
  "main": "./build/index.cjs",
  "module": "./src/index.ts",
  "typings": "./build/index.d.ts",
  "exports": {
    ".": {
      "@source": "./src/index.ts",
      "types": "./build/index.d.ts",
      "import": "./build/index.mjs",
      "require": "./build/index.cjs"
    },
    "./*": {
      "@source": "./src/*.ts",
      "types": "./build/*.d.ts",
      "import": "./build/*.mjs",
      "require": "./build/*.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/colyseus/colyseus.git"
  },
  "keywords": [
    "colyseus"
  ],
  "author": "Endel Dreyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/colyseus/colyseus/issues"
  },
  "files": [
    "build",
    "src"
  ],
  "homepage": "https://colyseus.io",
  "publishConfig": {
    "access": "public"
  }
}
