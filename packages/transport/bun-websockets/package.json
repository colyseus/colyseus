{
  "name": "@colyseus/bun-websockets",
  "version": "0.17.1",
  "type": "module",
  "input": "./src/index.ts",
  "main": "./build/index.js",
  "module": "./src/index.ts",
  "typings": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./src/index.ts",
      "require": "./build/index.js"
    },
    "./*": {
      "types": "./build/*.d.ts",
      "import": "./src/*.ts",
      "require": "./build/*.js"
    }
  },
  "dependencies": {
    "@colyseus/core": "workspace:^"
  },
  "devDependencies": {
    "@colyseus/core": "workspace:^",
    "bun-types": "^1.2.0"
  },
  "author": "Endel Dreyer",
  "license": "MIT",
  "keywords": [
    "colyseus",
    "transport",
    "websockets",
    "bun"
  ],
  "files": [
    "build",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/colyseus/colyseus.git"
  },
  "homepage": "https://colyseus.io/",
  "publishConfig": {
    "access": "public"
  }
}
