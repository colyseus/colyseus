{
  "name": "@colyseus/ws-transport",
  "version": "0.17.8",
  "type": "module",
  "input": "./src/index.ts",
  "main": "./build/index.cjs",
  "module": "./src/index.ts",
  "typings": "./build/index.d.ts",
  "exports": {
    ".": {
      "@source": "./src/index.ts",
      "types": "./build/index.d.ts",
      "import": "./build/index.mjs",
      "require": "./build/index.cjs"
    },
    "./*": {
      "@source": "./src/*.ts",
      "types": "./build/*.d.ts",
      "import": "./build/*.mjs",
      "require": "./build/*.cjs"
    }
  },
  "dependencies": {
    "@types/ws": "^8.5.14",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@colyseus/core": "workspace:^"
  },
  "peerDependencies": {
    "@colyseus/core": "0.17.x",
    "express": "^4.16.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  },
  "author": "Endel Dreyer",
  "license": "MIT",
  "keywords": [
    "colyseus",
    "transport",
    "websockets"
  ],
  "files": [
    "build",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/colyseus/colyseus.git"
  },
  "homepage": "https://colyseus.io/",
  "publishConfig": {
    "access": "public"
  },
