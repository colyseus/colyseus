{
  "name": "@colyseus/postgres-driver",
  "version": "0.16.0",
  "type": "module",
  "input": "./src/index.ts",
  "main": "./build/index.js",
  "module": "./src/index.ts",
  "typings": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./src/index.ts",
      "require": "./build/index.js"
    },
    "./*": {
      "types": "./build/*.d.ts",
      "import": "./src/*.ts",
      "require": "./build/*.js"
    }
  },
  "dependencies": {
    "@colyseus/core": "workspace:^",
    "zodgres": "^0.4.6",
    "zod": "^4.0.14"
  },
  "author": "Endel Dreyer",
  "license": "MIT",
  "keywords": [
    "colyseus",
    "matchmaking",
    "driver",
    "postgres",
    "sql"
  ],
  "files": [
    "build",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/colyseus/colyseus.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://colyseus.io/"
}
