{
  "name": "@colyseus/drizzle-driver",
  "version": "0.17.4",
  "type": "module",
  "input": "./src/index.ts",
  "main": "./build/index.cjs",
  "module": "./src/index.ts",
  "typings": "./build/index.d.ts",
  "exports": {
    ".": {
      "@source": "./src/index.ts",
      "types": "./build/index.d.ts",
      "import": "./build/index.mjs",
      "require": "./build/index.cjs"
    },
    "./*": {
      "@source": "./src/*.ts",
      "types": "./build/*.d.ts",
      "import": "./build/*.mjs",
      "require": "./build/*.cjs"
    }
  },
  "dependencies": {
    "@colyseus/core": "workspace:^",
    "drizzle-orm": "^0.38.4",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "drizzle-kit": "^0.30.6"
  },
  "peerDependencies": {
    "drizzle-orm": "^0.38.4",
    "drizzle-kit": "^0.30.6"
  },
  "author": "Endel Dreyer",
  "license": "MIT",
  "keywords": [
    "colyseus",
    "matchmaking",
    "driver",
    "drizzle"
  ],
  "files": [
    "build",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/colyseus/colyseus.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://colyseus.io/"
}
