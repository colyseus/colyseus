{
  "name": "@colyseus/auth",
  "version": "0.17.5",
  "type": "module",
  "description": "Authentication Utilities for Colyseus",
  "input": "./src/index.ts",
  "main": "./build/index.cjs",
  "module": "./src/index.ts",
  "typings": "./build/index.d.ts",
  "exports": {
    ".": {
      "@source": "./src/index.ts",
      "types": "./build/index.d.ts",
      "import": "./build/index.mjs",
      "require": "./build/index.cjs"
    },
    "./*": {
      "@source": "./src/*.ts",
      "types": "./build/*.d.ts",
      "import": "./build/*.mjs",
      "require": "./build/*.cjs"
    }
  },
  "scripts": {
    "test": "TS_NODE_PROJECT=../../tsconfig/tsconfig.test.json mocha test/**.test.ts test/**/**.test.ts --exit --timeout 15000"
  },
  "funding": "https://github.com/sponsors/endel",
  "author": "Endel Dreyer",
  "license": "MIT",
  "keywords": [
    "colyseus",
    "authentication",
    "auth",
    "oauth"
  ],
  "files": [
    "build",
    "src",
    "html",
    "oauth_help_urls.json"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/colyseus/colyseus.git"
  },
  "homepage": "https://colyseus.io/",
  "engines": {
    "node": ">= 22.x"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.5",
    "connect-redis": "^7.1.0",
    "express-jwt": "^8.5.1",
    "express-session": "^1.17.3",
    "grant": "^5.4.24",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@colyseus/better-call": "workspace:^",
    "@colyseus/core": "workspace:^",
    "httpie": "^2.0.0-next.13"
  },
  "peerDependencies": {
    "@colyseus/core": "0.17.x",
    "express": ">=4.16.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
