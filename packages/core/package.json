{
  "name": "@colyseus/core",
  "version": "0.17.34",
  "description": "Multiplayer Framework for Node.js.",
  "type": "module",
  "input": "./src/index.ts",
  "main": "./build/index.cjs",
  "module": "./src/index.ts",
  "typings": "./build/index.d.ts",
  "exports": {
    ".": {
      "@source": "./src/index.ts",
      "types": "./build/index.d.ts",
      "import": "./build/index.mjs",
      "require": "./build/index.cjs"
    },
    "./*": {
      "@source": "./src/*.ts",
      "types": "./build/*.d.ts",
      "import": "./build/*.mjs",
      "require": "./build/*.cjs"
    },
    "./package.json": "./package.json"
  },
  "funding": "https://github.com/sponsors/endel",
  "author": "Endel Dreyer",
  "license": "MIT",
  "keywords": [
    "colyseus",
    "multiplayer",
    "netcode",
    "realtime",
    "networking",
    "websockets"
  ],
  "files": [
    "build",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/colyseus/colyseus.git"
  },
  "homepage": "https://colyseus.io/",
  "engines": {
    "node": ">= 22.x"
  },
  "dependencies": {
    "@colyseus/shared-types": "workspace:^",
    "@colyseus/better-call": "workspace:^",
    "@colyseus/greeting-banner": "workspace:^",
    "@colyseus/msgpackr": "^1.11.2",
    "@colyseus/timer": "^2.0.0",
    "@standard-schema/spec": "^1.0.0",
    "debug": "^4.3.4",
    "nanoid": "^3.3.11"
  },
  "devDependencies": {
    "@colyseus/schema": "^4.0.7",
    "@colyseus/redis-driver": "workspace:^",
    "@colyseus/redis-presence": "workspace:^",
    "@colyseus/tools": "workspace:^",
    "express": "^5.0.0"
  },
  "peerDependencies": {
    "@colyseus/better-call": "workspace:^",
    "@colyseus/ws-transport": "workspace:^",
    "@colyseus/schema": "^4.0.7",
    "@pm2/io": "^6.1.0",
    "express": "^4.16.0 || ^5.0.0",
    "zod": "^4.1.12"
  },
  "optionalPeerDependencies": {
    "@colyseus/tools": "0.17.x",
    "@colyseus/redis-driver": "0.17.x",
    "@colyseus/redis-presence": "0.17.x"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "c45b410e99eadffff4b74e701339992e2faa15f8"
}
