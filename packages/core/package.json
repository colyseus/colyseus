{
  "name": "@colyseus/core",
  "version": "0.17.0",
  "description": "Multiplayer Framework for Node.js.",
  "type": "module",
  "input": "./src/index.ts",
  "main": "./build/index.js",
  "module": "./src/index.ts",
  "typings": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./src/index.ts",
      "require": "./build/index.js"
    },
    "./*": {
      "types": "./build/*.d.ts",
      "import": "./src/*.ts",
      "require": "./build/*.js"
    }
  },
  "funding": "https://github.com/sponsors/endel",
  "author": "Endel Dreyer",
  "license": "MIT",
  "keywords": [
    "colyseus",
    "multiplayer",
    "netcode",
    "realtime",
    "networking",
    "websockets"
  ],
  "files": [
    "build",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/colyseus/colyseus.git"
  },
  "homepage": "https://colyseus.io/",
  "engines": {
    "node": ">= 22.x"
  },
  "dependencies": {
    "@colyseus/better-call": "workspace:^",
    "@colyseus/greeting-banner": "workspace:^",
    "@colyseus/clock": "^1.0.1",
    "@colyseus/timer": "^1.0.3",
    "@colyseus/msgpackr": "^1.11.2",
    "@standard-schema/spec": "^1.0.0",
    "debug": "^4.3.4",
    "nanoid": "^3.3.11"
  },
  "devDependencies": {
    "@colyseus/schema": "^3.0.69"
  },
  "peerDependencies": {
    "@colyseus/schema": "^3.0.69",
    "@pm2/io": "^6.1.0",
    "zod": "^4.1.12"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "c45b410e99eadffff4b74e701339992e2faa15f8"
}
