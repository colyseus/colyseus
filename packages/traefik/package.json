{
  "name": "@colyseus/traefik",
  "version": "0.17.1",
  "type": "module",
  "description": "Utility to expose Colyseus servers to Traefik as a load balancer",
  "input": "./src/index.ts",
  "main": "./build/index.js",
  "module": "./src/index.ts",
  "typings": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./src/index.ts",
      "require": "./build/index.js"
    },
    "./*": {
      "types": "./build/*.d.ts",
      "import": "./src/*.ts",
      "require": "./build/*.js"
    }
  },
  "scripts": {
    "test": "TS_NODE_PROJECT=../../tsconfig/tsconfig.test.json mocha test/**.test.ts test/**/**.test.ts --exit --timeout 15000"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/colyseus/colyseus.git"
  },
  "keywords": [
    "colyseus"
  ],
  "author": "Endel Dreyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/colyseus/colyseus/issues"
  },
  "files": [
    "build",
    "src"
  ],
  "homepage": "https://colyseus.io",
  "dependencies": {
    "@colyseus/core": "workspace:^",
    "@colyseus/redis-presence": "workspace:^"
  },
  "devDependencies": {
    "@colyseus/ws-transport": "workspace:^"
  },
  "publishConfig": {
    "access": "public"
  }
}
