{
  "name": "@colyseus/loadtest",
  "version": "0.16.1",
  "description": "Utility tool for load testing Colyseus.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./src/index.ts",
      "require": "./build/index.js"
    },
    "./*": {
      "types": "./build/*.d.ts",
      "import": "./src/*.ts",
      "require": "./build/*.js"
    }
  },
  "input": "./src/index.ts",
  "main": "./build/index.js",
  "module": "./src/index.ts",
  "typings": "./build/index.d.ts",
  "bin": {
    "colyseus-loadtest": "./bin.js"
  },
  "files": [
    "build",
    "bin.js",
    "bin.mjs",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/colyseus/colyseus.git"
  },
  "homepage": "https://colyseus.io/",
  "keywords": [
    "colyseus",
    "load",
    "test",
    "load",
    "testing"
  ],
  "author": "Endel Dreyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/colyseus/colyseus/issues"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "colyseus.js": "^0.16.0",
    "minimist": "^1.2.5",
    "typescript": "^5.8.3",
    "ws": "^7.4.5"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.10"
  },
  "gitHead": "c45b410e99eadffff4b74e701339992e2faa15f8"
}
