{
  "private": true,
  "name": "@colyseus/legacy-schema-serializer",
  "version": "0.17.0",
  "type": "module",
  "input": "./src/index.ts",
  "main": "./build/index.js",
  "module": "./src/index.ts",
  "typings": "./build/index.d.ts",
  "exports": {
    ".": {
      "@source": "./src/index.ts",
      "types": "./build/index.d.ts",
      "import": "./build/index.mjs",
      "require": "./build/index.js"
    },
    "./*": {
      "@source": "./src/*.ts",
      "types": "./build/*.d.ts",
      "import": "./build/*.mjs",
      "require": "./build/*.js"
    }
  },
  "dependencies": {
    "@colyseus/core": "workspace:^",
    "@colyseus/schema": "^4.0.1"
  },
  "author": "Endel Dreyer",
  "license": "MIT",
  "files": [
    "build",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/colyseus/colyseus.git"
  },
  "homepage": "https://colyseus.io/",
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "320a0325f01eff3cd7afa8fb2efa80077ebdbf4a"
}
